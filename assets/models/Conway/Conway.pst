ate">newState	| nbOfAlives |	nbOfAlives := (self neighbourhood select: [: n | n state = #alive]) size.	self state = #dead &amp; (nbOfAlives = 3) ifTrue: [^self bufferState: #alive].	(self state = #alive and: [nbOfAlives = 2 or: [nbOfAlives = 3]])		ifTrue: [^self bufferState: #alive].	^self bufferState: #dead</body></methods><methods><class-id>CormasNS.Models.Conway.Cell</class-id> <category>init</category><body package="Conway" selector="initAllDead">initAllDead	self state: #dead.</body><body package="Conway" selector="initRandomly">initRandomly	Cormas random &lt; 0.5		ifTrue: [self state: #dead]		ifFalse: [self state: #alive]</body><body package="Conway" selector="initTen">initTen	" 10 % to be dead"	Cormas random &lt; 0.1		ifTrue: [self state: #dead]		ifFalse: [self state: #alive]</body></methods><methods><class-id>CormasNS.Models.Conway.Cell</class-id> <category>pov</category><body package="Conway" selector="pdv">pdv	^state</body></methods><methods><class-id>CormasNS.Models.Conway.Cell</class-id> <category>probes</category><body package="Conway" selector="isAlive">isAlive	self state = #alive ifTrue: [^1].	^0</body></methods><methods><class-id>CormasNS.Models.Conway.Cell class</class-id> <category>pov symbols</category><body package="Conway" selector="SpatialEntityPOV_alive">SpatialEntityPOV_alive"Returns an array containing the settings of a Spatial entity pov symbol in the following order:(color red) (color green) (color blue)"^Array with: 0.0 with: 0.670004 with: 0.0</body><body package="Conway" selector="SpatialEntityPOV_dead">SpatialEntityPOV_dead"Returns an array containing the settings of a Spatial entity pov symbol in the following order:(color red) (color green) (color blue)"^Array with: 0.0 with: 0.0 with: 0.0</body></methods><methods><class-id>CormasNS.Models.Conway.Cell class</class-id> <category>- default value</category><body package="Conway" selector="state_default">state_default 	"Return the default value of state attribute"		^nil</body></methods><methods><class-id>CormasNS.Models.Conway.Conway</class-id> <category>init</category><body package="Conway" selector="init">init"set the neighbour to 8 keeping the current state of the cells"	self spaceModel setNbNeighbours: 8.</body><body package="Conway" selector="initRandomly">initRandomly	"set the neighbour to 8 and initialize the cells randomly"		self spaceModel setNbNeighbours: 8.	self initCells: #initRandomly</body></methods><methods><class-id>CormasNS.Models.Conway.Conway</class-id> <category>control</category><body package="Conway" selector="stepSynchronously:">stepSynchronously: t	super stepSynchronously: t</body></methods><methods><class-id>CormasNS.Models.Conway.Conway</class-id> <category>probes</category><body package="Conway" selector="alive">alive	^(self theCells select: [: c | c state = #alive]) size / self theCells size</body><body package="Conway" selector="dead">dead	^(self theCells select: [: c | c state = #dead]) size / self theCells size</body></methods><methods><class-id>CormasNS.Models.Conway.Conway</class-id> <category>accessing</category><body package="Conway" selector="theCells">theCells^theCells ifNil:[theCells := IndexedSet new]</body><body package="Conway" selector="theCells:">theCells: coll	theCells := coll</body></methods><methods><class-id>CormasNS.Models.Conway.Conway class</class-id> <category>description</category><body package="Conway" selector="cormasCompatibleVersion">cormasCompatibleVersion^ #(2014 2 24 7 27 12)</body><body package="Conway" selector="lastUpdate">lastUpdate^ 'March 4, 2014'	</body><body package="Conway" selector="version">version^ 'Conway.pcl'	</body><body package="Conway" selector="vwCompatibleVersion">vwCompatibleVersion^ '7.6'	</body></methods><do-it>"Imported Classes:"</do-it><do-it>self error: 'Attempting to file-in parcel imports.  Choose terminate or close'</do-it><class><na